{
  "manifestVersion": 1,
  "id": "loadimpactvnext",
  "name": "Load Impact Extension VSTS Vnext",
  "version": "0.2.0",
  "publisher": "julienstroheker",
  "description": "Launch your Load Test from Load Impact",
  "categories": [
      "Test"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "icon.png"
  },
  "tags": [
        "test",
        "load test",
        "load impact",
        "loadimpact"
    ],
  "files": [
    {
      "path": "LoadImpact"
    }
  ],
  "contributions": [
      {
          "id": "loadimpact-task",
          "type": "ms.vss-distributed-task.task",
          "targets": [
              "ms.vss-distributed-task.tasks"
          ],
          "properties": {
              "name": "LoadImpact"
          }
      }
  ]
}
