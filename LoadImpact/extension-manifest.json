{
  "manifestVersion": 1,
  "id": "loadimpact",
  "name": "Load Impact Extension",
  "version": "1.1.1",
  "publisher": "julienstroheker",
  "description": "Trigger your Load Test from Load Impact",
  "public": true,
  "categories": [
      "Test"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "icon.png"
  },
  "tags": [
        "test",
        "load test",
        "load impact",
        "loadimpact"
    ],
  "content": {
        "details": {
            "path": "overview.md"
        }
    },
  "links": {
        "repository": {
            "uri": "https://github.com/julienstroheker/loadimpact-vsts-extension"
        },
        "issues": {
            "uri": "https://github.com/julienstroheker/loadimpact-vsts-extension/issues"
        }
    },
  "repository": {
        "type": "git",
        "uri": "https://github.com/julienstroheker/loadimpact-vsts-extension"
    },
  "branding": {
        "color": "rgb(255, 255, 255)",
        "theme": "light"
    },
  "files": [
    {
      "path": "LoadImpact"
    }
  ],
  "contributions": [
      {
          "id": "loadimpact-task",
          "type": "ms.vss-distributed-task.task",
          "targets": [
              "ms.vss-distributed-task.tasks"
          ],
          "properties": {
              "name": "LoadImpact"
          }
      }
  ]
}
